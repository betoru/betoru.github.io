<span class="k">if</span> <span class="o">(</span><span class="n">n</span> <span class="o">!=</span> <span class="mi">1</span><span class="o">)</span> <span class="o">{</span>
    <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"1μ΄ μ•„λ‹λ„¤?"</span><span class="o">);</span>
<span class="o">}</span>

<span class="o">-&gt;</span> <span class="kc">false</span>
<span class="o">-&gt;</span> <span class="mi">1</span><span class="n">μ΄</span> <span class="n">μ•„λ‹λ„¤</span><span class="o">?</span>
